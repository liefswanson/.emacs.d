;ergoemacs goof up
(global-set-key (kbd "M-3") 'split-window-horizontally)

;company nav
(global-set-key (kbd "M-c") 'ac-company-up)
(global-set-key (kbd "M-t") 'ac-company-down)

(defun ac-company-up()
  "handles navigation up in auto-complete and company"
  (ac-previous)
  )

(defun ac-company-down()
  "handles navigation down in auto-complete and company"
  (ac-next)
  )


(provide 'bindings-conf)
;;; bindings-conf.el ends here
